<%= form_for @project do |f| %>
    <%= f.hidden_field :user_id, value: current_user.id %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :language %>
    <%= f.text_field :language %><br>

    <%= f.label :description %>
    <%= f.text_field :description %><br>

    <%= f.label :url %>
    <%= f.text_field :url %><br>

    <%= f.submit %>
<% end %>
